# UZFOOD Backend Environment Variables
# Copy this file to .env and fill in your values

# Telegram Bot Token (get from @BotFather on Telegram)
TOKEN=YOUR_BOT_TOKEN_HERE

# Admin Telegram IDs (comma-separated, e.g., 7961099561,1234567890)
ADMIN_IDS=7961099561

# Admin password for panel authentication (change to strong password!)
ADMIN_PW=megaadmin

# WebApp URL (where frontend is hosted, e.g., GitHub Pages)
WEB_APP=https://your-github-username.github.io/uzfood/

# Backend API URL (for development: http://127.0.0.1:8080)
API_URL=http://127.0.0.1:8080
